{
  "name": "ember-cli-bugsnag",
  "version": "1.4.1",
  "description": "Integrates Bugsnag reporting service into your Ember CLI app.",
  "scripts": {
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 ./*.js addon app config tests",
    "start": "ember serve",
    "test": "COVERAGE=true ember test",
    "build": "ember build",
    "test:all": "ember try:each",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 100 --functions 100 --branches 100 --lines 100",
    "commit": "git-cz",
    "posttest": "npm run report-coverage && npm run check-coverage && npm run validate-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "semantic-release": "semantic-release",
    "validate-coverage": "test \"$(cat coverage/coverage-summary.json | json total.lines.total)\" -gt 0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@ember/optional-features": "^1.1.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "ember-auto-import": "^1.5.2",
    "ember-cli": "~3.14.0",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-circleci": "0.1.0",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-test-loader": "^2.1.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^6.0.0",
    "ember-source": "~3.14.1",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.3.0",
    "eslint-config-bbva": "^3.0.0",
    "eslint-plugin-ember": "^7.6.0",
    "eslint-plugin-node": "^10.0.0",
    "husky": "^3.1.0",
    "istanbul": "^0.4.5",
    "json": "^9.0.6",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.9.0",
    "semantic-release": "^15.13.31"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/binhums/ember-cli-bugsnag.git"
  },
  "license": "MIT",
  "author": "Alex Navasardyan, Ben Holmes",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "bugsnag-js": "^4.7.3",
    "ember-cli-babel": "^7.11.1",
    "ember-cli-htmlbars": "^4.0.5",
    "ember-cli-node-assets": "^0.2.2",
    "fastboot-transform": "^0.1.1",
    "npm": "^6.13.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
